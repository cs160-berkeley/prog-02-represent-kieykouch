<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.kieykouch.politicawesome.current_location"
    android:background="#ffffff">

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout"
        android:background="#f9f2a0"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true">

        <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:id="@+id/imageView"
            android:src="@drawable/barbara_boxer"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="-20dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Barbara"
            android:id="@+id/name_detail"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/imageView"
            android:textColor="#ff0000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Democrat"
            android:id="@+id/party_detail"
            android:layout_below="@+id/name_detail"
            android:layout_toEndOf="@+id/imageView"
            android:textColor="#ff0000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Senator"
            android:id="@+id/status_detail"
            android:layout_below="@+id/party_detail"
            android:layout_toEndOf="@+id/imageView"
            android:textColor="#000000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="jone@cena.com"
            android:id="@+id/email_detial"
            android:layout_below="@+id/status_detail"
            android:layout_toEndOf="@+id/imageView"
            android:textColor="#000000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="818 216 6424"
            android:id="@+id/phone"
            android:layout_below="@+id/email_detial"
            android:layout_toEndOf="@+id/imageView"
            android:textColor="#000000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Term_end"
            android:id="@+id/term"
            android:layout_alignBottom="@+id/imageView"
            android:layout_toEndOf="@+id/imageView"
            android:textColor="#000000" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Bill Record"
            android:id="@+id/textView12"
            android:layout_below="@+id/term"
            android:layout_alignParentStart="true"
            android:layout_marginTop="38dp"
            android:textColor="#006f06" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="04/13/2016 Gay Marriage"
            android:id="@+id/textView13"
            android:layout_below="@+id/textView12"
            android:layout_alignParentStart="true"
            android:textColor="#0025f6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="04/13/2016 Gay Marriage"
            android:id="@+id/textView14"
            android:layout_below="@+id/textView13"
            android:layout_alignParentStart="true"
            android:textColor="#0025f6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="04/13/2016 Gay Marriage"
            android:id="@+id/textView15"
            android:layout_below="@+id/textView14"
            android:layout_alignParentStart="true"
            android:textColor="#0025f6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="04/13/2016 Gay Marriage"
            android:id="@+id/textView16"
            android:layout_below="@+id/textView15"
            android:layout_alignParentStart="true"
            android:textColor="#0025f6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="04/13/2016 Gay Marriage"
            android:id="@+id/textView17"
            android:layout_below="@+id/textView16"
            android:layout_alignParentStart="true"
            android:textColor="#0025f6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Commitee"
            android:id="@+id/textView18"
            android:layout_marginTop="30dp"
            android:layout_below="@+id/textView17"
            android:layout_alignParentStart="true"
            android:textColor="#006f06" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Gay Association"
            android:id="@+id/textView19"
            android:layout_below="@+id/textView18"
            android:layout_alignParentStart="true"
            android:textColor="#0025f6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Gay Association"
            android:id="@+id/textView20"
            android:layout_below="@+id/textView19"
            android:layout_alignParentStart="true"
            android:textColor="#0025f6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Gay Association"
            android:id="@+id/textView21"
            android:layout_below="@+id/textView20"
            android:layout_alignParentStart="true"
            android:textColor="#0025f6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Gay Association"
            android:id="@+id/textView22"
            android:layout_below="@+id/textView21"
            android:layout_alignParentStart="true"
            android:textColor="#0025f6" />

    </RelativeLayout>





</RelativeLayout>
